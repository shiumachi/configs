IdentityFile ~/.ssh/github_rsa
IdentityFile ~/.ssh/google_compute_engine
IdentityFile ~/.ssh/gitlab_rsa
IdentityFile ~/.ssh/bitbucket_rsa

Host github.com
  User git
  Port 22
  Hostname github.com
  IdentityFile ~/.ssh/github_rsa
  TCPKeepAlive yes
  IdentitiesOnly yes

Host bitbucket.org
  User git
  Port 22
  Hostname bitbucket.org
  IdentityFile ~/.ssh/bitbucket_rsa
  TCPKeepAlive yes
  IdentitiesOnly yes
